{
  "name": "golf-shot-transcription",
  "version": "1.0.0",
  "description": "Golf shot transcription app with multimodal LLM",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "swarm": "node scripts/execute-swarm.js",
    "swarm:week1": "node backend/src/agents/ImplementationController.js week 1",
    "swarm:week2": "node backend/src/agents/ImplementationController.js week 2",
    "swarm:full": "node backend/src/agents/ImplementationController.js full",
    "swarm:report": "node backend/src/agents/ImplementationController.js report",
    "install:swarm": "npm install bull ioredis p-limit chalk ora prompts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --ci --watchAll=false --passWithNoTests",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:visual": "playwright test --project=visual-regression",
    "test:mobile": "playwright test --project=\"Mobile Chrome\" --project=\"Mobile Safari\"",
    "test:performance": "k6 run performance/__tests__/load-testing.spec.js",
    "test:all": "npm run test:ci && npm run test:e2e && npm run test:performance",
    "test:parallel": "concurrently \"npm run test:unit\" \"npm run test:integration\"",
    "test:forensic": "npm run test:all && npm run coverage:validate && npm run test:mathematical-precision",
    "test:mathematical-precision": "jest --testNamePattern=\"Mathematical|Precision|Calculation\"",
    "coverage:validate": "node scripts/validate-coverage.js",
    "coverage:report": "jest --coverage && opener coverage/lcov-report/index.html",
    "lint:test": "eslint **/__tests__/**/*.{js,ts,tsx} --fix",
    "format:test": "prettier --write **/__tests__/**/*.{js,ts,tsx}"
  },
  "devDependencies": {
    "bull": "^4.16.5",
    "chalk": "^4.1.2",
    "concurrently": "^8.2.2",
    "ioredis": "^5.6.1",
    "ora": "^5.4.1",
    "p-limit": "^4.0.0",
    "prompts": "^2.4.2",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.23.0",
    "@faker-js/faker": "^8.2.0",
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/testing-library__jest-dom": "^5.14.0",
    "allure-playwright": "^2.10.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.50.0",
    "eslint-plugin-jest": "^27.4.0",
    "eslint-plugin-playwright": "^0.18.0",
    "eslint-plugin-testing-library": "^6.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporters": "^3.1.0",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.0",
    "k6": "^0.47.0",
    "opener": "^1.5.2",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.0"
  }
}
